tellraw @s[scores={DialogueTrigger=177,TalkTime=304}] ["",{"text":"<"},{"text":"...","color":"yellow"},{"text":"> Well, but... But Mimi is sort of my family. We stick together."}]
tellraw @s[scores={DialogueTrigger=177,TalkTime=312}] ["",{"text":"<"},{"text":"Looker","color":"gray"},{"text":"> Indeed..."}]
tellraw @s[scores={DialogueTrigger=177,TalkTime=317}] ["",{"text":"<"},{"text":"Looker","color":"gray"},{"text":"> Indeed! I, Looker, have struck upon an idea!"}]
