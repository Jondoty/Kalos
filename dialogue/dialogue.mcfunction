tellraw @s[scores={DialogueTrigger=214,TalkTime=211}] ["",{"text":"<"},{"text":"Emma","color":"gold"},{"text":"> "},{"text":"Thank you for all your assistance... And adieu. P.S. To ","italic":true},{"selector":"@s","italic":true},{"text":", my colleague...","italic":true}]
