tellraw @s[scores={DialogueTrigger=177,TalkTime=365}] ["",{"text":"<"},{"text":"Looker","color":"gray"},{"text":"> It is not to imply that anything's wrong with a free life in the alleys, but..."}]
tellraw @s[scores={DialogueTrigger=177,TalkTime=375}] ["",{"text":"<"},{"text":"Looker","color":"gray"},{"text":"> To have a home to shelter you from the cold and the rain..."}]
tellraw @s[scores={DialogueTrigger=177,TalkTime=384}] ["",{"text":"<"},{"text":"Looker","color":"gray"},{"text":"> There is joy in having work to do each day and your efforts seen and appreciated."}]
tellraw @s[scores={DialogueTrigger=177,TalkTime=394}] ["",{"text":"<"},{"text":"Looker","color":"gray"},{"text":"> This idea is genius, even for me!"}]
